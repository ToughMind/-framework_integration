<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="lq.core.domain.po">
	<class name="lq.core.domain.po.UserPO" table="user">
		<id name="id" column="Id">
			<generator class="native"></generator>
		</id>
		<property name="name" column="name" type="java.lang.String"
			not-null="true" length="20"></property>
		<property name="status" column="status" type="lq.core.domain.bo.enums.UserStatusGenderType"
			not-null="true" length="255"></property>
		<property name="createTime" column="createTime" type="java.lang.Long"
			not-null="true"></property>
		<property name="updateTime" column="updateTime" type="java.lang.String"
			not-null="true"></property>
		<property name="price" column="price" type="java.lang.Double"
			not-null="true"></property>
		<property name="money" column="money" type="java.lang.Double"
			not-null="true"></property>
	</class>
</hibernate-mapping>  